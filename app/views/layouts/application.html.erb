<!DOCTYPE html>
<html>
<head>
  <title>Stock Portfolio</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash[:errors].each do |message| %>
    <h3> <%= message %> </h3>
  <% end if flash[:errors] %>

  <%= yield %>

</body>
</html>
